{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend for Looking 4 Mage",
  "main": "dist/app.js",
  "scripts": {
    "build": "webpack --config config/webpack/prod.ts",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint": "npm run -s lint:ts",
    "lint:ts": "eslint --format codeframe --ext .js,.jsx,.ts,.tsx .",
    "start": "webpack-dev-server --open --config config/webpack/dev.ts",
    "test": "npm run -s test:unit && npm run -s lint",
    "test:unit": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/looking4mage/frontend.git"
  },
  "keywords": [
    "looking",
    "for",
    "mage"
  ],
  "author": "Looking 4 Mage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/looking4mage/frontend/issues"
  },
  "homepage": "https://github.com/looking4mage/frontend#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.0",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/eslint": "^6.1.6",
    "@types/history": "^4.7.4",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.5.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.2",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/config/",
      "index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "<rootDir>/config/jest/file.ts"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "<rootDir>/src/**/*.test.+(ts|tsx|js|jsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/enzyme.config.ts"
    ]
  }
}
